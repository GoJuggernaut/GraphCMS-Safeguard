{
    "name": "GraphCMS Safeguard",
    "action": {},
    "manifest_version": 3,
    "version": "0.1",
    "description": "Protection from accidentally publishing changes on GraphCMS",
    "icons": { 
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "host_permissions": [
        "https://app.hygraph.com/*"
    ],
    "permissions": [
        "tabs",
        "activeTab",
        "scripting",
        "https://app.hygraph.com/*"
    ],
    "background": {
        "service_worker": "background.js"
    }
}
